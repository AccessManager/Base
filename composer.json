{
  "name": "access-manager/base",
  "description": "A sub-module for Access Manager",
  "keywords": ["wifi", "hotspot", "mikrotik"],
  "license": "MIT",
  "homepage": "http://accessmanager.in",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "^5.5.0",
    "laravelcollective/html": "^5.4.0",
    "spatie/url": "^1.1",
    "access-manager/auth": "dev-master",
    "access-manager/account-details": "dev-master",
    "access-manager/accounts": "dev-master",
    "access-manager/constants": "dev-master",
    "access-manager/helpers": "dev-master",
    "access-manager/prepaid": "dev-master",
    "access-manager/radius": "dev-master",
    "access-manager/routers": "dev-master",
    "access-manager/services": "dev-master",
    "access-manager/setup": "dev-master",
    "access-manager/zones": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "AccessManager\\Base\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}